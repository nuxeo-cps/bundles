<?xml version="1.0"?>
<bundle>

  <server name="CPS Products, mainline server"
	  url="http://hg.cps-cms.org"
	  push-url="https://hg.cps-cms.org">
    <branch path="products/CPSCompat" testing="continuous"/>
    <branch path="products/CPSCore"  testing="continuous"/>
    <branch path="products/CPSDefault" testing="continuous" />
    <branch path="products/CPSDesignerThemes" testing="continuous" />
    <branch path="products/CPSDirectory" testing="continuous" />
    <branch path="products/CPSDocument" testing="continuous" />
    <branch path="products/CPSI18n" testing="continuous" />
    <branch path="products/CPSNavigation" testing="continuous" />
    <branch path="products/CPSPortlets" testing="continuous" />
    <branch path="products/CPSSchemas" testing="continuous" />
    <branch path="products/CPSSubscriptions" testing="continuous" />
    <branch path="products/CPSUserFolder" testing="continuous" />
    <branch path="products/CPSUtil" testing="continuous" />
    <branch path="products/CPSWorkflow" testing="continuous" />
    <branch path="products/CPSonFive" testing="continuous" />

    <branch path="zope2/Localizer"  testing="continuous"/>
    <branch path="zope2/PortalTransforms" testing="yes" />
    <branch path="zope2/TranslationService" testing="continuous" />
    <branch path="zope2/ZMIntrospection"  testing="yes"/>
  </server>

  <!-- Other "vendor" products from Nuxeo's svn repository.
       These are live branches to allow quick fixes. -->
  <server url="http://hg.cps-cms.org/vendor"
	  push-url="https://hg.cps-cms.org/vendor">
    <branch target="ExternalEditor" path="ExternalEditor/0.9.2"/>
    <branch target = "FCKeditor" name="fckeditor.plone_2.2-nux"
	    path="FCKeditor/fckeditor.plone_2.2"/>
    <tag name="1.0.1-for-cmf-1.6.0" path="UnicodeLexicon"/>
  </server>

  <!-- Sporadic stuff -->
  <server url="http://hg.cps-cms.org"
          push-url="https://hg.cps-cms.org">
    <tag name="2.0.2" path="OOo/ooo2dbk"
         target="PortalTransforms/transforms/ooo2dbk"/>
  </server>

</bundle>
